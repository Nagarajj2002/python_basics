print("Graffy Academy")

phrase ="Graffy Academy"
print(phrase)
print(phrase + " is cool")

print(phrase.lower())
print(phrase.upper())
print(phrase.isupper())
print(phrase.upper().isupper())

print(len(phrase))
print(phrase[0])
print(phrase[4])

print(phrase.index("A"))
print(phrase.replace("Graffy","Elephant"))